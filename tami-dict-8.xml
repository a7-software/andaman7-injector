    <!---->

    <!-- Biometry Ami's-->
    <Ami id="ami.height" type="number" tags="tag.biometry">
        <Qualifier id="qualifier.unit" type="oneSelection" selectionListId="sl.unit.length" />
    </Ami>
    <Ami id="ami.weight" type="number" tags="tag.biometry">
        <Qualifier id="qualifier.unit" type="oneSelection" selectionListId="sl.unit.mass" />
    </Ami>
    <Ami id="ami.bmi" type="number" tags="tag.biometry">
        <Formula>
            height = [ami.height];
            return [ami.weight] / Math.pow(height,2);
        </Formula>
    </Ami>
    <Ami id="ami.waist" type="number" tags="tag.biometry">
        <Qualifier id="qualifier.unit" type="oneSelection" selectionListId="sl.unit.length" />
    </Ami>
    <Ami id="ami.hips" type="number" tags="tag.biometry">
        <Qualifier id="qualifier.unit" type="oneSelection" selectionListId="sl.unit.length" />
    </Ami>
    <Ami id="ami.waistHipsRatio" type="number" tags="tag.biometry">
        <Formula>
            return [ami.waist] / [ami.hips];
        </Formula>
    </Ami>
    <Ami id="ami.adiposity" type="number" tags="tag.biometry" />
    <Ami id="ami.fever" type="number" tags="tag.biometry">
        <Qualifier id="qualifier.unit" type="oneSelection" selectionListId="sl.unit.temperature"  />
    </Ami>
    <Ami id="ami.tension" type="string" tags="tag.biometry">
        <Qualifier id="qualifier.date" type="date" />
        <Qualifier id="qualifier.result" type="string" />
    </Ami>
    <!---->

    <!-- Habits Ami's-->
    <Ami id="ami.smokerFrequency" type="oneSelection" selectionListId="sl.smokerFrequency" tags="tag.habit">
        <Qualifier id="qualifier.startDate" type="date" />
        <Qualifier id="qualifier.quantity" type="number" />
