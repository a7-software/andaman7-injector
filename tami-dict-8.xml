    </Ami>
    <Ami id="ami.kids" type="number" tags="tag.administrative" />
    <Ami id="ami.bloodGroup" type="oneSelection" selectionListId="sl.bloodGroup" tags="tag.administrative" />

    <Ami id="ami.birthDate" type="date" tags="tag.administrative" />
