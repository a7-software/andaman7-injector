    <Ami id="ami.drug" type="oneSelection" selectionListId="sl.drugAddiction" tags="tag.habit">
        <Qualifier id="qualifier.type" type="multiSelection" selectionListId="sl.drugType" />
        <Qualifier id="qualifier.quantity" type="oneSelection" selectionListId="sl.drugQuantity" />
    </Ami>
    <Ami id="ami.sport" type="oneSelection" selectionListId="sl.sportFrequency" tags="tag.habit">
        <Qualifier id="qualifier.type" type="multiSelection" selectionListId="sl.sportType" />
        <Qualifier id="qualifier.startDate" type="date" />
    </Ami>
    <!---->

