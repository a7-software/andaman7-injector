<!-- Definition of Ami's -->

    <!-- EHR AmiSet -->
    <AmiSet id="amiSet.ehr" type="none" >
        <Marker id="marker.amiContainer" />
        <Marker id="marker.hidden" />
        <Marker id="marker.multi" />
    </AmiSet>
    <Ami id="ami.namespaceEntry" type="string" >
        <Qualifier id="qualifier.namespaceValue" type="string" />
        <Marker id="marker.hidden"/>
        <Marker id="marker.multi" />
    </Ami>

    <!-- Administrative Ami's -->
