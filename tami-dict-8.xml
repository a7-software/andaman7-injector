            <Item id="li.stone" ratio="6.35029" system="us" />
            <Item id="li.pound" ratio="0.453592" system="us" default="true" />
            <Item id="li.ounce" ratio="0.0283495" system="us" />
            <Item id="li.dram" ratio="0.0017718452" system="us" />
