    <Ami id="ami.mobilePhone" type="string" tags="tag.administrative" />
    <Ami id="ami.fax" type="string" tags="tag.administrative" />

    <Ami id="ami.doctorFunction" type="multiSelection" selectionListId="sl.doctorFunction" tags="tag.administrative" />
    <Ami id="ami.function" type="oneSelection" selectionListId="sl.regime" tags="tag.administrative" />
    <Ami id="ami.employ" type="string" tags="tag.administrative" >
        <Qualifier type="string" id="qualifier.employer" />
        <Qualifier type="date" id="qualifier.startDate" />
        <Qualifier type="date" id="qualifier.endDate" />
    </Ami>
    <!---->

    <!-- Contacts Ami's-->
    <Ami id="ami.doctor" type="string" tags="tag.contact" >
        <Qualifier id="qualifier.speciality" type="string" />
