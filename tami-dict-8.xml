        <Qualifier id="qualifier.phone" type="string" />
        <Qualifier id="qualifier.mobilePhone" type="string" />
        <Qualifier id="qualifier.mail" type="string" />

        <Marker id="marker.multi" />
    </Ami>
    <Ami id="ami.emergencyContact" type="string" tags="tag.contact">
        <Qualifier id="qualifier.relation" type="oneSelection" selectionListId="sl.relationship" />
        <Qualifier id="qualifier.phone" type="string" />
        <Qualifier id="qualifier.phone2" type="string" />
        <Qualifier id="qualifier.email" type="string" />
        <Qualifier id="qualifier.address" type="string" />

        <Marker id="marker.multi" />
    </Ami>
    <!---->

    <!-- Insurances Ami's-->
    <Ami id="ami.mutualFundation" type="string" tags="tag.insurance">
        <Qualifier id="qualifier.startDate" type="date" />
        <Qualifier id="qualifier.endDate" type="date" />
        <Marker id="marker.multi" />
    </Ami>
    <Ami id="ami.insurance" type="string" tags="tag.insurance">
        <Qualifier id="qualifier.startDate" type="date" />
        <Qualifier id="qualifier.endDate" type="date" />

